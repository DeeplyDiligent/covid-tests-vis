{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "purge:css": "cross-env NODE_ENV=production tailwindcss build styles/tailwind.css -o styles/index.css",
    "build:css": "tailwindcss build styles/tailwind.css -o styles/index.css",
    "watch:css": "tailwindcss build styles/tailwind.css -o styles/index.css -w",
    "start": "npm run build:css && next start",
    "build": "npm run purge:css && next build",
    "dev": "npm run purge:css && next dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "chart.js": "^2.9.3",
    "csv-parser": "^2.3.3",
    "http": "^0.0.1-security",
    "moment": "^2.27.0",
    "next": "9.5.1",
    "papaparse": "^5.3.0",
    "react": "16.13.1",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "16.13.1",
    "react-icons": "^3.11.0",
    "swr": "^0.3.2",
    "tailwindcss": "^1.8.10",
    "xhr2": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "typescript": "^4.0.2",
    "autoprefixer": "^9.8.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10"
  }
}
